<p class="p5"><span class="s1"><span class="Apple-converted-space">            </span></span><span class="s3">&lt;/</span><span class="s6">div</span><span class="s3">&gt;</span></p>
<p class="p5"><span class="s1"><span class="Apple-converted-space">        </span></span><span class="s3">&lt;/</span><span class="s6">section</span><span class="s3">&gt;</span></p>
<p class="p7"><span class="s9"></span><br></p>
<p class="p5"><span class="s1"><span class="Apple-converted-space">    </span></span><span class="s3">&lt;/</span><span class="s6">main</span><span class="s3">&gt;</span></p>
<p class="p7"><span class="s9"></span><br></p>
<p class="p6"><span class="s4"><span class="Apple-converted-space">    </span></span><span class="s1">&lt;!-- FOOTER --&gt;</span></p>
<p class="p4"><span class="s4"><span class="Apple-converted-space">    </span></span><span class="s3">&lt;</span><span class="s6">footer</span><span class="s4"> </span><span class="s2">class</span><span class="s3">=</span><span class="s1">"bg-brand-black text-brand-white mt-16"</span><span class="s3">&gt;</span></p>
<p class="p4"><span class="s4"><span class="Apple-converted-space">        </span></span><span class="s3">&lt;</span><span class="s6">div</span><span class="s4"> </span><span class="s2">class</span><span class="s3">=</span><span class="s1">"container mx-auto px-6 py-6 text-center"</span><span class="s3">&gt;</span></p>
<p class="p5"><span class="s1"><span class="Apple-converted-space">            </span></span><span class="s3">&lt;</span><span class="s6">p</span><span class="s3">&gt;</span><span class="s1">&amp;copy; 2024 Nina Digital by GO THE EXTRA MILE. All Rights Reserved.</span><span class="s3">&lt;/</span><span class="s6">p</span><span class="s3">&gt;</span></p>
<p class="p5"><span class="s1"><span class="Apple-converted-space">        </span></span><span class="s3">&lt;/</span><span class="s6">div</span><span class="s3">&gt;</span></p>
<p class="p2"><span class="s4"><span class="Apple-converted-space">    </span></span><span class="s3">&lt;/</span><span class="s1">footer</span><span class="s3">&gt;</span></p>
<p class="p7"><span class="s9"></span><br></p>
<p class="p6"><span class="s4"><span class="Apple-converted-space">    </span></span><span class="s1">&lt;!-- JAVASCRIPT สำหรับการทำงานของ TAB และ Mobile Menu --&gt;</span></p>
<p class="p2"><span class="s4"><span class="Apple-converted-space">    </span></span><span class="s3">&lt;</span><span class="s1">script</span><span class="s3">&gt;</span></p>
<p class="p6"><span class="s4"><span class="Apple-converted-space">        </span></span><span class="s1">// ฟังก์ชันสำหรับสลับการแสดงผลของ Tab Content</span></p>
<p class="p5"><span class="s1"><span class="Apple-converted-space">        </span></span><span class="s11">function</span><span class="s1"> showTab(tabId, element) {</span></p>
<p class="p6"><span class="s4"><span class="Apple-converted-space">            </span></span><span class="s1">// ซ่อน Tab Content ทั้งหมด</span></p>
<p class="p5"><span class="s1"><span class="Apple-converted-space">            </span>document.querySelectorAll(</span><span class="s7">'.tab-content'</span><span class="s1">).forEach(</span><span class="s11">function</span><span class="s1">(tab) {</span></p>
<p class="p5"><span class="s1"><span class="Apple-converted-space">                </span>tab.classList.remove(</span><span class="s7">'active'</span><span class="s1">);</span></p>
<p class="p5"><span class="s1"><span class="Apple-converted-space">            </span>});</span></p>
<p class="p6"><span class="s4"><span class="Apple-converted-space">            </span></span><span class="s1">// แสดง Tab Content ที่เลือก</span></p>
<p class="p5"><span class="s1"><span class="Apple-converted-space">            </span>document.getElementById(tabId).classList.add(</span><span class="s7">'active'</span><span class="s1">);</span></p>
<p class="p7"><span class="s9"></span><br></p>
<p class="p6"><span class="s4"><span class="Apple-converted-space">            </span></span><span class="s1">// ทำให้เมนูที่เลือกอยู่เป็น Active (เปลี่ยนสี)</span></p>
<p class="p5"><span class="s1"><span class="Apple-converted-space">            </span>document.querySelectorAll(</span><span class="s7">'.nav-link'</span><span class="s1">).forEach(</span><span class="s11">function</span><span class="s1">(link) {</span></p>
<p class="p5"><span class="s1"><span class="Apple-converted-space">                </span>link.classList.remove(</span><span class="s7">'active'</span><span class="s1">);</span></p>
<p class="p5"><span class="s1"><span class="Apple-converted-space">            </span>});</span></p>
<p class="p7"><span class="s1"><span class="Apple-converted-space">            </span></span></p>
<p class="p6"><span class="s4"><span class="Apple-converted-space">            </span></span><span class="s1">// ใช้ querySelectorAll เพื่อให้ครอบคลุมทั้ง desktop และ mobile menu ที่อาจถูกคลิก</span></p>
<p class="p6"><span class="s4"><span class="Apple-converted-space">            </span></span><span class="s1">// เราจะหาทุก link ที่มี href ชี้ไปยัง tabId ที่ต้องการ</span></p>
<p class="p5"><span class="s1"><span class="Apple-converted-space">            </span>document.querySelectorAll(</span><span class="s7">'a[href="#'</span><span class="s1"> + tabId + </span><span class="s7">'"]'</span><span class="s1">).forEach(</span><span class="s11">function</span><span class="s1">(link) {</span></p>
<p class="p5"><span class="s1"><span class="Apple-converted-space">                </span>link.classList.add(</span><span class="s7">'active'</span><span class="s1">);</span></p>
<p class="p5"><span class="s1"><span class="Apple-converted-space">            </span>});</span></p>
<p class="p7"><span class="s9"></span><br></p>
<p class="p6"><span class="s4"><span class="Apple-converted-space">            </span></span><span class="s1">// เลื่อนหน้าจอไปด้านบนสุด</span></p>
<p class="p5"><span class="s1"><span class="Apple-converted-space">             </span>window.scrollTo({ top: </span><span class="s12">0</span><span class="s1">, behavior: </span><span class="s7">'smooth'</span><span class="s1"> });</span></p>
<p class="p5"><span class="s1"><span class="Apple-converted-space">        </span>}</span></p>
<p class="p7"><span class="s9"></span><br></p>
<p class="p6"><span class="s4"><span class="Apple-converted-space">        </span></span><span class="s1">// การทำงานของ Mobile Menu</span></p>
<p class="p5"><span class="s1"><span class="Apple-converted-space">        </span></span><span class="s11">const</span><span class="s1"> mobileMenuButton = document.getElementById(</span><span class="s7">'mobile-menu-button'</span><span class="s1">);</span></p>
<p class="p5"><span class="s1"><span class="Apple-converted-space">        </span></span><span class="s11">const</span><span class="s1"> mobileMenu = document.getElementById(</span><span class="s7">'mobile-menu'</span><span class="s1">);</span></p>
<p class="p5"><span class="s1"><span class="Apple-converted-space">        </span>mobileMenuButton.addEventListener(</span><span class="s7">'click'</span><span class="s1">, </span><span class="s11">function</span><span class="s1">() {</span></p>
<p class="p5"><span class="s1"><span class="Apple-converted-space">            </span>mobileMenu.classList.toggle(</span><span class="s7">'hidden'</span><span class="s1">);</span></p>
<p class="p5"><span class="s1"><span class="Apple-converted-space">        </span>});</span></p>
<p class="p7"><span class="s9"></span><br></p>
<p class="p5"><span class="s1"><span class="Apple-converted-space">        </span></span><span class="s11">function</span><span class="s1"> closeMobileMenu() {</span></p>
<p class="p5"><span class="s1"><span class="Apple-converted-space">             </span>mobileMenu.classList.add(</span><span class="s7">'hidden'</span><span class="s1">);</span></p>
<p class="p5"><span class="s1"><span class="Apple-converted-space">        </span>}</span></p>
<p class="p7"><span class="s9"></span><br></p>
<p class="p6"><span class="s4"><span class="Apple-converted-space">        </span></span><span class="s1">// ทำให้ Tab แรก (HOME) แสดงผลเมื่อเปิดหน้าเว็บ</span></p>
<p class="p5"><span class="s1"><span class="Apple-converted-space">        </span>document.addEventListener(</span><span class="s7">'DOMContentLoaded'</span><span class="s1">, </span><span class="s11">function</span><span class="s1">() {</span></p>
<p class="p5"><span class="s1"><span class="Apple-converted-space">            </span>showTab(</span><span class="s7">'home'</span><span class="s1">, document.querySelector(</span><span class="s7">'.nav-link'</span><span class="s1">));</span></p>
<p class="p5"><span class="s1"><span class="Apple-converted-space">        </span>});</span></p>
<p class="p2"><span class="s4"><span class="Apple-converted-space">    </span></span><span class="s3">&lt;/</span><span class="s1">script</span><span class="s3">&gt;</span></p>
<p class="p7"><span class="s9"></span><br></p>
<p class="p2"><span class="s3">&lt;/</span><span class="s1">body</span><span class="s3">&gt;</span></p>
<p class="p2"><span class="s3">&lt;/</span><span class="s1">html</span><span class="s3">&gt;</span></p>
<p class="p7"><span class="s9"></span><br></p>
<p class="p7"><span class="s9"></span><br></p>
</body>
